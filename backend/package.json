{
  "name": "web_project_around_express",
  "version": "1.0.0",
  "description": "Back-end focused on creating a server for the Around The U.S. Project",
  "main": "app.js",
  "engines": {
    "node": ">=22.15.0"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "dev:open": "node -e \"require('open')('http://localhost:3000')\" && npm run dev",
    "prepare": "husky",
    "lint": "npx eslint .",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "bcryptjs": "3.0.3",
    "dotenv": "17.2.3",
    "express": "5.1.0",
    "mongoose": "8.18.0",
    "validator": "13.15.23"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.10",
    "open": "^7.4.2",
    "prettier": "^3.7.0"
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint:fix",
    "*.{json,md}": "npm run format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VanessaYuriAB/web_project_around_express.git"
  },
  "keywords": [
    "study",
    "back-end",
    "api",
    "server",
    "node",
    "nodemon",
    "express",
    "javascript",
    "eslint",
    "airbnb",
    "open",
    "around",
    "us",
    "web",
    "project"
  ],
  "author": "Vanessa Yuri A. Brito <emaildavanessayuri@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VanessaYuriAB/web_project_around_express/issues"
  },
  "homepage": "https://github.com/VanessaYuriAB/web_project_around_express#readme"
}
